{
  "name": "base-dash",
  "version": "1.0.0",
  "description": "base dash - endless runner game for base mini app",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "analyze": "ANALYZE=true next build",
    "compile": "hardhat compile",
    "deploy:base-sepolia": "hardhat run scripts/deploy.js --network baseSepolia",
    "deploy:base": "hardhat run scripts/deploy.js --network base",
    "verify": "hardhat verify --network baseSepolia",
    "test": "hardhat test",
    "full-deploy:testnet": "npm run compile && npm run deploy:base-sepolia",
    "full-deploy:mainnet": "npm run compile && npm run deploy:base"
  },
  "dependencies": {
    "@coinbase/onchainkit": "^1.1.2",
    "@openzeppelin/contracts": "^5.0.0",
    "@tanstack/react-query": "^5.24.0",
    "next": "^14.2.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-jsx": "^5.1.7",
    "viem": "^2.8.0",
    "wagmi": "^2.5.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.21",
    "@nomicfoundation/hardhat-chai-matchers": "^2.1.0",
    "@nomicfoundation/hardhat-ethers": "^3.1.3",
    "@nomicfoundation/hardhat-network-helpers": "^1.1.2",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@nomicfoundation/hardhat-verify": "^2.1.3",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/chai": "^4.3.20",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.17",
    "chai": "^4.5.0",
    "dotenv": "^16.4.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.2.21",
    "ethers": "^6.16.0",
    "hardhat": "^2.20.0",
    "hardhat-gas-reporter": "^1.0.10",
    "postcss": "^8.4.35",
    "solidity-coverage": "^0.8.17",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typechain": "^8.3.2",
    "typescript": "^5.3.0"
  }
}
