# Приложение
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Контракт (обновляется после деплоя)
NEXT_PUBLIC_CONTRACT_ADDRESS=0x0000000000000000000000000000000000000000

# Альтернативные адреса для разных сетей
NEXT_PUBLIC_CONTRACT_ADDRESS_MAINNET=0x0000000000000000000000000000000000000000
NEXT_PUBLIC_CONTRACT_ADDRESS_TESTNET=0x0000000000000000000000000000000000000000

# Сеть (true для testnet, false для mainnet)
NEXT_PUBLIC_USE_TESTNET=true

# MiniKit (для Base Mini App)
NEXT_PUBLIC_MINIKIT_APP_ID=

# OnchainKit (получите на https://portal.cdp.coinbase.com/)
NEXT_PUBLIC_ONCHAINKIT_API_KEY=

# WalletConnect Project ID (получите на https://cloud.walletconnect.com/)
NEXT_PUBLIC_WALLET_CONNECT_PROJECT_ID=

# Приватный ключ для деплоя контракта и gasless (НИКОГДА не коммитьте в git!)
PRIVATE_KEY=

# API ключ для Basescan (для верификации контракта)
BASESCAN_API_KEY=

# Приватный ключ для подписи очков (требуется для score-sign API)
# Если не указан, используется PRIVATE_KEY
SCORE_SIGNER_PRIVATE_KEY=
